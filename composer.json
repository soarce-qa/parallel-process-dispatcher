{
    "name": "soarce/parallel-process-dispatcher",
    "description": "Tiny PHP library for running jobs in background and/or in parallel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Holger Segnitz",
            "email": "holger@segnitz.net"
        }
    ],
    "readme": "README.md",
    "config": {
        "platform": {
            "php": "5.3.9"
        }
    },
    "require": {
        "php": ">=5.3"
    },
    "autoload": {
        "psr-4": {
            "Soarce\\ParallelProcessDispatcher\\": "src/"
        }
    },
    "replace": {
      "fastbill/parallel-process-dispatcher": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "jakub-onderka/php-parallel-lint": "*",
        "phploc/phploc": "^2.1",
        "pdepend/pdepend": "^2.5",
        "phpmd/phpmd": "*",
        "squizlabs/php_codesniffer": "*",
        "sebastian/phpcpd": "^2.0",
        "theseer/phpdox": "0.8.1.1"
    }
}
